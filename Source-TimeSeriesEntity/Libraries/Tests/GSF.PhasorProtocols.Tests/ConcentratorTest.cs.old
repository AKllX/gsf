#region [ Using ]
using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using GSF.TestsSuite.PhasorProtocols.Wrappers.IEEE.C37118;
using GSF.TestsSuite.PhasorProtocols.Cases.IEEE.C37118;
using GSF.PhasorProtocols.IEEEC37_118;
using Microsoft.VisualStudio.TestTools.UnitTesting;
#endregion

namespace GSF.PhasorProtocols.Tests
{
    /// <summary>
    ///This is a test class for ConcentratorTest and is intended
    ///to contain all ConcentratorTest Unit Tests
    ///</summary>
    [TestClass()]
    public class ConcentratorTest
    {

        #region [ Members ]
        private ConfigurationCellCase m_ConfigurationCellCase;
        #endregion

        #region [ Context ]
        private TestContext testContextInstance;

        /// <summary>
        ///Gets or sets the test context which provides
        ///information about and functionality for the current test run.
        ///</summary>
        public TestContext TestContext
        {
            get
            {
                return testContextInstance;
            }
            set
            {
                testContextInstance = value;
            }
        }
        #endregion

        #region [ Additional test attributes ]
        //
        //You can use the following additional attributes as you write your tests:
        //
        //Use ClassInitialize to run code before running the first test in the class
        //[ClassInitialize()]
        //public static void MyClassInitialize(TestContext testContext)
        //{
        //}
        //
        //Use ClassCleanup to run code after all tests in a class have run
        //[ClassCleanup()]
        //public static void MyClassCleanup()
        //{
        //}

        //Use TestInitialize to run code before running each test
        [TestInitialize()]
        public void MyTestInitialize()
        {
            m_ConfigurationCellCase = new ConfigurationCellCase();
        }

        //Use TestCleanup to run code after each test has run
        [TestCleanup()]
        public void MyTestCleanup()
        {
        }
        #endregion

        #region [ Methods ]
        /// <summary>
        ///A test for Concentrator Constructor
        ///</summary>
        [TestMethod()]
        public void ConcentratorConstructorTest()
        {
            GSF.PhasorProtocols.IEEEC37_118.Concentrator target = new GSF.PhasorProtocols.IEEEC37_118.Concentrator();
            Assert.IsInstanceOfType(target, typeof(Concentrator));
            Assert.IsNotNull(target);
        }

        /// <summary>
        ///A test for CreateConfigurationFrame
        ///</summary>
        [TestMethod()]
        public void CreateConfigurationFrameTest()
        {
            GSF.PhasorProtocols.Anonymous.ConfigurationFrame baseConfigurationFrame =  m_ConfigurationCellCase.ConfigurationFrame;
            uint timeBase = 0;
            GSF.PhasorProtocols.LineFrequency nominalFrequency = new GSF.PhasorProtocols.LineFrequency();
            GSF.PhasorProtocols.IEEEC37_118.ConfigurationFrame2 expected = (ConfigurationFrame2)m_ConfigurationCellCase.ConfigurationFrame2;
            GSF.PhasorProtocols.IEEEC37_118.ConfigurationFrame2 actual;
            actual = GSF.PhasorProtocols.IEEEC37_118.Concentrator.CreateConfigurationFrame(baseConfigurationFrame, timeBase, nominalFrequency);
            Assert.AreEqual(expected, actual);
        }

        /// <summary>
        ///A test for CreateDataFrame
        ///</summary>
        [TestMethod()]
        public void CreateDataFrameTest()
        {
            GSF.PhasorProtocols.IEEEC37_118.ConfigurationFrame1 configurationFrame = (ConfigurationFrame1)m_ConfigurationCellCase.ConfigurationFrame1;
            GSF.PhasorProtocols.IEEEC37_118.DataFrame expected = m_ConfigurationCellCase.DataFrame;
            GSF.PhasorProtocols.IEEEC37_118.DataFrame actual;
            actual = GSF.PhasorProtocols.IEEEC37_118.Concentrator.CreateDataFrame(m_ConfigurationCellCase.TimeStamp, configurationFrame);
            Assert.AreEqual(expected, actual);
        }

        /// <summary>
        ///A test for Initialize
        ///</summary>
        [TestMethod()]
        public void InitializeTest()
        {
            try
            {
                m_ConfigurationCellCase.Concentrator.Initialize();
                Assert.IsTrue(true);
            }
            catch
            {
                Assert.IsTrue(false);
            }
        }

        /// <summary>
        ///A test for Status
        ///</summary>
        [TestMethod()]
        public void StatusTest()
        {
            GSF.PhasorProtocols.IEEEC37_118.Concentrator target = m_ConfigurationCellCase.Concentrator;
            string actual;
            actual = target.Status;
            Assert.AreNotEqual("", target.Status);
        }

        /// <summary>
        ///A test for TimeBase
        ///</summary>
        [TestMethod()]
        public void TimeBaseTest()
        {
            GSF.PhasorProtocols.IEEEC37_118.Concentrator target = m_ConfigurationCellCase.Concentrator;
            uint expected = 0;
            uint actual;
            target.TimeBase = expected;
            actual = target.TimeBase;
            Assert.AreEqual(expected, actual);
        }

        /// <summary>
        ///A test for ValidateIDCode
        ///</summary>
        [TestMethod()]
        public void ValidateIDCodeTest()
        {
            GSF.PhasorProtocols.IEEEC37_118.Concentrator target = m_ConfigurationCellCase.Concentrator;
            bool expected = false;
            bool actual;
            target.ValidateIDCode = expected;
            actual = target.ValidateIDCode;
            Assert.AreEqual(expected, actual);
        }
        #endregion
    }
}
