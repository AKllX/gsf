<Project DefaultTargets="Build" xmlns="http://schemas.microsoft.com/developer/msbuild/2003" ToolsVersion="4.0">
  <PropertyGroup>
    <!-- The configuration and platform will be used to determine which
         assemblies to include from solution and project documentation
         sources -->
    <Configuration Condition=" '$(Configuration)' == '' ">Debug</Configuration>
    <Platform Condition=" '$(Platform)' == '' ">AnyCPU</Platform>
    <SchemaVersion>2.0</SchemaVersion>
    <ProjectGuid>{f20c4e07-742b-4a26-989a-8ff93477236a}</ProjectGuid>
    <SHFBSchemaVersion>1.9.0.0</SHFBSchemaVersion>
    <!-- AssemblyName, Name, and RootNamespace are not used by SHFB but Visual
         Studio adds them anyway -->
    <AssemblyName>Documentation</AssemblyName>
    <RootNamespace>Documentation</RootNamespace>
    <Name>Documentation</Name>
    <!-- SHFB properties -->
    <OutputPath>..\Output\$(Configuration)\Libraries\Help\</OutputPath>
    <HtmlHelpName>TVACodeLibrary</HtmlHelpName>
    <HelpTitle>TVA Code Library Help</HelpTitle>
    <DocumentationSources>
      <DocumentationSource sourceFile="..\Output\$(Configuration)\Libraries\TVA.Communication.dll" />
      <DocumentationSource sourceFile="..\Output\$(Configuration)\Libraries\TVA.Communication.xml" />
      <DocumentationSource sourceFile="..\Output\$(Configuration)\Libraries\TVA.Core.dll" />
      <DocumentationSource sourceFile="..\Output\$(Configuration)\Libraries\TVA.Core.xml" />
      <DocumentationSource sourceFile="..\Output\$(Configuration)\Libraries\TVA.Security.dll" />
      <DocumentationSource sourceFile="..\Output\$(Configuration)\Libraries\TVA.Security.xml" />
      <DocumentationSource sourceFile="..\Output\$(Configuration)\Libraries\TVA.ServiceBus.dll" />
      <DocumentationSource sourceFile="..\Output\$(Configuration)\Libraries\TVA.ServiceBus.xml" />
      <DocumentationSource sourceFile="..\Output\$(Configuration)\Libraries\TVA.ServiceModel.dll" />
      <DocumentationSource sourceFile="..\Output\$(Configuration)\Libraries\TVA.ServiceModel.xml" />
      <DocumentationSource sourceFile="..\Output\$(Configuration)\Libraries\TVA.ServiceProcess.dll" />
      <DocumentationSource sourceFile="..\Output\$(Configuration)\Libraries\TVA.ServiceProcess.xml" />
      <DocumentationSource sourceFile="..\Output\$(Configuration)\Libraries\TVA.Web.dll" />
      <DocumentationSource sourceFile="..\Output\$(Configuration)\Libraries\TVA.Web.xml" />
      <DocumentationSource sourceFile="..\Output\$(Configuration)\Libraries\TVA.Windows.dll" />
      <DocumentationSource sourceFile="..\Output\$(Configuration)\Libraries\TVA.Windows.xml" />
    </DocumentationSources>
    <FrameworkVersion>4.0.30319</FrameworkVersion>
    <Preliminary>False</Preliminary>
    <PresentationStyle>Prototype</PresentationStyle>
    <MissingTags>Summary, Parameter, Returns, AutoDocumentCtors, Namespace, TypeParameter</MissingTags>
    <CopyrightText>Tennessee Valley Authority, 2010. No copyright is claimed pursuant to 17 USC &amp;#167%3b 105.  All Other Rights Reserved.</CopyrightText>
    <CopyrightHref>http://tvacodelibrary.codeplex.com/license</CopyrightHref>
    <FeedbackEMailAddress>pcpatel%40tva.gov</FeedbackEMailAddress>
    <FeedbackEMailLinkText>Pinal C. Patel</FeedbackEMailLinkText>
    <VisibleItems>Attributes, InheritedMembers, InheritedFrameworkMembers, Protected, SealedProtected</VisibleItems>
    <PlugInConfigurations>
      <PlugInConfig id="Table of Contents Exclusion" enabled="True">
        <configuration />
      </PlugInConfig>
    </PlugInConfigurations>
    <HelpFileFormat>HtmlHelp1, MSHelp2</HelpFileFormat>
    <KeepLogFile>False</KeepLogFile>
  </PropertyGroup>
  <!-- There are no properties for these two groups but they need to appear in
       order for Visual Studio to perform the build. -->
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Debug|AnyCPU' ">
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Release|AnyCPU' ">
  </PropertyGroup>
  <!-- Import the SHFB build targets -->
  <Import Project="$(SHFBROOT)\SandcastleHelpFileBuilder.targets" />
</Project>